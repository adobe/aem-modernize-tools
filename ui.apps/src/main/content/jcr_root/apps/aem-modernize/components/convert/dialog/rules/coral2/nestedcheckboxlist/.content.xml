<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <patterns jcr:primaryType="nt:unstructured">
        <nestedcheckboxlist jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/foundation/form/nestedcheckboxlist">
            <items jcr:primaryType="nt:unstructured"/>
        </nestedcheckboxlist>
    </patterns>
    <replacement
        jcr:primaryType="nt:unstructured"
        cq:rewriteCommonAttrs="{Boolean}true"
        cq:rewriteRenderCondition="{Boolean}true">
        <nestedcheckboxlist
            cq:rewriteFinal="{Boolean}true"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/nestedcheckboxlist"
            disconnected="${./disconnected}">
            <items
                cq:rewriteMapChildren="./items"
                jcr:primaryType="nt:unstructured"/>
        </nestedcheckboxlist>
    </replacement>
</jcr:root>
