<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    cq:rewriteRanking="3">
    <patterns jcr:primaryType="nt:unstructured">
        <well jcr:primaryType="nt:unstructured"
              sling:resourceType="granite/ui/components/foundation/well">
            <items jcr:primaryType="nt:unstructured"
                   cq:rewriteOptional="{Boolean}true"/>
        </well>
    </patterns>
    <replacement
        jcr:primaryType="nt:unstructured"
        cq:rewriteCommonAttrs="{Boolean}true"
        cq:rewriteRenderCondition="{Boolean}true">
        <well
            cq:rewriteFinal="{Boolean}true"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/well">
            <items
                cq:rewriteFinal="{Boolean}true"
                cq:rewriteMapChildren="./items"
                jcr:primaryType="nt:unstructured"/>
        </well>
    </replacement>
</jcr:root>
