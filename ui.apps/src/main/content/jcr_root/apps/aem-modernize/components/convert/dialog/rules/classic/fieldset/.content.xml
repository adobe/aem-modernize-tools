<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <patterns jcr:primaryType="nt:unstructured">
        <dialogfieldset  jcr:primaryType="cq:Widget"
                         xtype="dialogfieldset">
            <items jcr:primaryType="cq:WidgetCollection"
                   cq:rewriteOptional="{Boolean}true"/>
        </dialogfieldset>
        <fieldset jcr:primaryType="cq:Widget"
                  xtype="fieldset">
            <items jcr:primaryType="cq:WidgetCollection"
               cq:rewriteOptional="{Boolean}true"/>
        </fieldset>
    </patterns>
    <replacement jcr:primaryType="nt:unstructured">
        <fieldset
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
            jcr:title="${./title}"
            cq:rewriteFinal="{Boolean}true">
            <items
                jcr:primaryType="nt:unstructured"
                cq:rewriteMapChildren="./items"
                cq:rewriteFinal="{Boolean}true"/>
        </fieldset>
    </replacement>
</jcr:root>
