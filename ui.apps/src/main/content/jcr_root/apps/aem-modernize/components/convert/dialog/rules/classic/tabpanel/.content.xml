<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    cq:rewriteRanking="3">
    <patterns jcr:primaryType="nt:unstructured">
        <tabpanel jcr:primaryType="cq:TabPanel">
            <items jcr:primaryType="cq:WidgetCollection"
                   cq:rewriteOptional="{Boolean}true"/>
        </tabpanel>
        <tabpanel1 jcr:primaryType="cq:Widget"
                   xtype="tabpanel">
            <items jcr:primaryType="cq:WidgetCollection"
                   cq:rewriteOptional="{Boolean}true"/>
        </tabpanel1>
        <tabpanel2 jcr:primaryType="nt:unstructured"
                   xtype="tabpanel">
            <items jcr:primaryType="cq:WidgetCollection"
                   cq:rewriteOptional="{Boolean}true"/>
        </tabpanel2>
    </patterns>
    <replacement jcr:primaryType="nt:unstructured">
        <tabs
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/tabs"
            cq:rewriteFinal="{Boolean}true">
            <items
                jcr:primaryType="nt:unstructured"
                cq:rewriteMapChildren="./items"
                cq:rewriteFinal="{Boolean}true"/>
        </tabs>
    </replacement>
</jcr:root>
