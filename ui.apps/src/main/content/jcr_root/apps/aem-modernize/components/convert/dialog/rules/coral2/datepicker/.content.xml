<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <patterns jcr:primaryType="nt:unstructured">
        <datepicker jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/form/datepicker"/>
    </patterns>
    <replacement
        jcr:primaryType="nt:unstructured"
        cq:rewriteFinal="{Boolean}true"
        cq:rewriteCommonAttrs="{Boolean}true"
        cq:rewriteRenderCondition="{Boolean}true">
        <datepicker
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/form/datepicker"
            fieldLabel="${./fieldLabel}"
            fieldDescription="${./fieldDescription}"
            renderHidden="${./renderHidden}"
            name="${./name}"
            value="${./value}"
            emptyText="${./emptyText}"
            disabled="${./disabled}"
            required="${./required}"
            validation="${./validation}"
            type="${./type}"
            displayedFormat="${./displayedFormat}"
            valueFormat="${./storedFormat}"
            minDate="${./minDate}"
            maxDate="${./maxDate}"
            displayTimezoneMessage="${./displayTimezoneMessage}"/>
    </replacement>
</jcr:root>
